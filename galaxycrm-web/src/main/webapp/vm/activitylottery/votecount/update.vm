 </script>


<h2 class="contentTitle">修改增量信息</h2>
<form method="post" action="${basePath}/activity/votecount/update" class="required-validate pageForm" onsubmit="return validateCallback(this, dialogAjaxDone);">
	<input type="hidden" name="id" value="${item.id}"/>
	<div class="pageFormContent" layoutH="97">
	
		<dl>
			<dt>活动编号：</dt>
			<dd>
				<input type="text" name="activityId" class="required"  size="32"  value="$!{item.activityId}"/>
			</dd>
		</dl>
		
		
		<dl>
			<dt>视频编号：</dt>
			<dd>
				<input type="text" name="videoId" class="required"  size="32"   value="$!{item.videoId}"/>
			</dd>
		</dl>
		

		<dl>
			<dt>视频名称：</dt>
			<dd>
				<input type="text" name="videoName" class="required"  size="32"   value="$!{item.videoName}"/>
			</dd>
		</dl>
		<dl>
		
		
		<dt>投票数量：</dt>
			<dd>
				<input type="text" name="count" class="digits"  size="32"   value="$!{item.count}" />
			</dd>
		</dl>
		
		
		<dt>增量：</dt>
			<dd>
				<input type="text" name="countAdd" class="digits"  size="32"   value="$!{item.countAdd}" />
			</dd>
		</dl>
	     
	     
       
				
	</div>
			
	<div class="formBar">
		<ul>
			<li><div class="buttonActive"><div class="buttonContent"><button type="submit">确定</button></div></div></li>
			<li><div class="button"><div class="buttonContent"><button type="button" class="close">关闭</button></div></div></li>
		</ul>
	</div>
</form>