<h2 class="contentTitle">添加模块</h2>
<form method="post" action="${basePath}/management/security/setting/create" class="required-validate pageForm" onsubmit="return validateCallback(this, dialogAjaxDone);">
	<input type="hidden" name="parentId" value="${parentModule.id}"/>
	<div class="pageFormContent" layoutH="97">
		<dl>
			<dt>key：</dt>
			<dd>
				<input type="text" name="name" class="required" size="32" maxlength="32"/>
			</dd>
		</dl>
		<dl>
			<dt>value：</dt>
			<dd>
				<input type="text" name="data" class="required" size="32" maxlength="32" value="" />
			</dd>
		</dl>		
		<dl>
			<dt>描述：</dt>
			<dd>
				<input type="text" name="remark"  size="32" maxlength="255" />
			</dd>
		</dl>	
	</div>
			
	<div class="formBar">
		<ul>
			<li><div class="buttonActive"><div class="buttonContent"><button type="submit">确定</button></div></div></li>
			<li><div class="button"><div class="buttonContent"><button type="button" class="close">关闭</button></div></div></li>
		</ul>
	</div>
</form>